{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist/frontend/browser",
  "routes": [
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/ngsw-worker.js",
      "dest": "/ngsw-worker.js"
    },
    {
      "src": "^/(assets|styles.*)",
      "dest": "/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
